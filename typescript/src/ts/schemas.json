{
    "string-schema": {
      "type": "string",
      "minLength": 2,
      "maxLength": 50,
      "pattern": "^[A-Za-z]+$"
    },
  
    "number-schema": {
      "type": "number",
      "minimum": 0,
      "maximum": 100,
      "multipleOf": 0.5
    },
  
    "integer-schema": {
      "type": "integer",
      "minimum": 1,
      "maximum": 10
    },
  
    "boolean-schema": {
      "type": "boolean"
    },
  
    "null-schema": {
      "type": "null"
    },
  
    "array-schema": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "minItems": 1,
      "maxItems": 5,
      "uniqueItems": true
    },
  
    "enum-schema": {
      "type": "string",
      "enum": ["red", "green", "blue"]
    },
  
    "const-schema": {
      "const": "fixed value"
    },
  
    "multi-type-schema": {
      "type": ["string", "number"]
    },
  
    "allOf-schema": {
      "allOf": [
        { "type": "number" },
        { "multipleOf": 5 }
      ]
    },
  
    "anyOf-schema": {
      "anyOf": [
        { "type": "string", "maxLength": 5 },
        { "type": "number", "minimum": 0 }
      ]
    },
  
    "oneOf-schema": {
      "oneOf": [
        { "type": "number", "multipleOf": 5 },
        { "type": "number", "multipleOf": 3 }
      ]
    },
  
    "not-schema": {
      "not": {
        "type": "string"
      }
    }
  }